{
    "issuer": "Udemy",
    "desc": "Platform for online learning and teaching with courses in various subjects.",
    "website": "https://www.udemy.com",
    "APIs": [
      {
        "host": "www.udemy.com",
        "intercept": {
          "url": "/api-2.0/contexts/users/me",
          "method": "GET"
        },
        "assert": [
          {
            "key": "header|isLoggedIn",
            "value": "true",
            "operation": "="
          }
        ],
        "nullifier": "user_id"
      }
    ],
    "HRCondition": [
      "Udemy Account Owner"
    ],
    "tips": {
      "message": "Please log in to your Udemy account and click the 'Start' button to verify your account ownership."
    }
  }