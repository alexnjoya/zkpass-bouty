{
    "issuer": "Binance",
    "desc": "Binance KYC age verification schema",
    "website": "https://www.binance.com",
    "APIs": [
        {
            "host": "www.binance.com",
            "intercept": {
                "url": "bapi/kyc/v1/private/kyc/user-kyc/get-personal-detail",
                "method": "GET"
            },
            "assert": [
                {
                    "key": "birthday",
                    "value": "2005-11-12",
                    "operation": "<="
                }
            ],
            "nullifier": "birthday"
        }
    ],
    "HRCondition": [
        "User must be at least 18 years old"
    ],
    "tips": {
        "message": "Please ensure that your birthday is correctly entered to verify your age."
    }
}