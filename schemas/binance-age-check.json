{
    "issuer": "Binance",
    "desc": "Binance KYC age verification schema",
    "website": "https://www.binance.com/en/my/dashboard",
    "APIs": [
      {
        "host": "www.binance.com",
        "intercept": {
          "url": "bapi/kyc/v2/private/certificate/user-kyc/current-kyc-status",
          "method": "POST"
        },
        "assert": [
          {
            "key": "data|fillInfo|country",
            "value": "GH",
            "operation": "=="
          }
        ],
        "nullifier": "data|fillInfo|country"
      }
    ],
    "HRCondition": [
      "User must be at least 18 years old"
    ],
    "tips": {
      "message": "Please ensure that your birthday is correctly entered to verify your age."
    }
  }